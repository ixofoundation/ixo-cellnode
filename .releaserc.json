{
  "branches": [
    "main",
    {
      "name": "develop",
      "channel": "alpha",
      "prerelease": true
    },
    {
      "name": "test",
      "channel": "beta",
      "prerelease": true
    }
  ],
  "ci": true,
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/git",
      {
        "assets": ["package.json"]
      }
    ],
    "@semantic-release/github"
  ]
}