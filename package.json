{
  "name": "ixo-cellnode",
  "version": "0.3.0-develop.2",
  "description": "ixo service to create a project data store and more",
  "repository": "https://github.com/ixofoundation/ixo-cellnode",
  "main": "App.ts",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node ./build/dist/server.js",
