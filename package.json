    "dotenv": "^6.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "ipfs-only-hash": "^4.0.0",
    "jayson": "^2.1.2",
    "node": "^18.15.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.19",
    "typescript": "5.3.3",
    "web3.storage": "^4.4.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
